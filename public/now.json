{
    "version": 2,
    "name": "training-tracker",
    "alias": "trainingtracker.mwillard.now.sh",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "^/favicon.ico$",
          "dest": "/favicon.ico"
         },
         {
          "src": "^/manifest.json$",
          "dest": "/manifest.json"
         },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ]
  }